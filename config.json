{   
    "data":{
        "dataset":"mnist",
        "data_directory":"downloaded_datasets",
        "ratio":[0.8,0.1,0.1],
        "transforms": {
            "name":"default",
            "options":{
                "default": [
                    {"name": "Resize", "params": {"height": 256, "width": 256}},
                    {"name": "CenterCrop", "params": {"height": 224, "width": 224}},
                    {"name": "ToTensor"}
                ],
                "inception_v3": [
                    {"name": "Resize", "params": {"height": 299, "width": 299}},
                    {"name": "CenterCrop", "params": {"height": 299, "width": 299}},
                    {"name": "ToTensor"}
                ],
                "vit": [
                    {"name": "Resize", "params": {"height": 384, "width": 384}},
                    {"name": "CenterCrop", "params": {"height": 384, "width": 384}},
                    {"name": "ToTensor"}
                ]
            }
        }
    },
    "hyperparams":{
        "loss":{
            "name":"cross_entropy"
        },
        "optimizer":{
            "optimizer_name":"sgd",
            "learning_rate":0.0001
        },
        "network":"deep_cnn",
        "batch_size":64,
        "epochs":10,
        "seed":0
    },
    "networks":{
        "fcn":{
            "filters":[512,256,128]
        },
        "deep_cnn":{
            "cnn": [
                {
                    "filters": 128,
                    "kernel_size": 3,
                    "stride": 1,
                    "padding": 1,
                    "pool": 2,
                    "norm": "batch",
                    "activation": "relu"
                }
            ],
            "output_layer": {
                "dropout": 0.1
            }
        },
        "pretrained":{
            "name":"googlenet"
        }
    },
    "evaluation":{
        "metrics":["accuracy","precision","f1","recall"]
    },
    "logging":{
        "interval":10,
        "wandb":{
            "project":"Pytorch_MNIST_Classification"
        },
        "tensorboard":{
            "logdir":"tensorboard_logs"
        }
    }
    
}