{
    "experiment": {
        "experiment_name": "run_7",
        "experiment_dir": "bacil_experiments/run_7"
    },
    "data": {
        "dataset": "custom",
        "data_directory": "/home/julian/indonezia/dataset/classification_dataset_julian",
        "ratio": [
            0.8,
            0.1,
            0.1
        ],
        "transforms": {
            "name": "based_on_paper",
            "options": {
                "default": [
                    {
                        "name": "Resize",
                        "params": {
                            "height": 256,
                            "width": 256
                        }
                    },
                    {
                        "name": "CenterCrop",
                        "params": {
                            "height": 224,
                            "width": 224
                        }
                    },
                    {
                        "name": "ToTensor"
                    }
                ],
                "based_on_paper": [
                    {
                        "name": "RandomRotate90",
                        "params": {
                            "p": 0.5
                        }
                    },
                    {
                        "name": "ShiftScaleRotate",
                        "params": {
                            "shift_limit": 0.05,
                            "scale_limit": 0.1,
                            "rotate_limit": 0,
                            "p": 0.5
                        }
                    },
                    {
                        "name": "RandomBrightnessContrast",
                        "params": {
                            "brightness_limit": 0.2,
                            "contrast_limit": 0.2,
                            "p": 0.5
                        }
                    },
                    {
                        "name": "HorizontalFlip",
                        "params": {
                            "p": 0.5
                        }
                    },
                    {
                        "name": "Resize",
                        "params": {
                            "height": 224,
                            "width": 224
                        }
                    },
                    {
                        "name": "ToTensor"
                    }
                ],
                "inception_v3": [
                    {
                        "name": "Resize",
                        "params": {
                            "height": 299,
                            "width": 299
                        }
                    },
                    {
                        "name": "CenterCrop",
                        "params": {
                            "height": 299,
                            "width": 299
                        }
                    },
                    {
                        "name": "ToTensor"
                    }
                ],
                "vit": [
                    {
                        "name": "Resize",
                        "params": {
                            "height": 384,
                            "width": 384
                        }
                    },
                    {
                        "name": "CenterCrop",
                        "params": {
                            "height": 384,
                            "width": 384
                        }
                    },
                    {
                        "name": "ToTensor"
                    }
                ],
                "custom": [
                    {
                        "name": "ToTensor"
                    }
                ]
            }
        },
        "input_shape": [
            224,
            224,
            3
        ],
        "num_classes": 2
    },
    "hyperparams": {
        "loss": {
            "name": "cross_entropy"
        },
        "optimizer": {
            "optimizer_name": "adamw",
            "scheduler": {
                "name": "cosineannealinglr",
                "params": {
                    "T_max": 2,
                    "eta_min": 1e-05,
                    "last_epoch": -1
                }
            },
            "learning_rate": 0.0001,
            "weight_decay": 0.0005
        },
        "network": "pretrained",
        "batch_size": 128,
        "epochs": 2,
        "seed": 0,
        "early_stopping_patience": 5
    },
    "networks": {
        "fcn": {
            "filters": [
                512,
                256,
                128
            ]
        },
        "deep_cnn": {
            "layers": [
                {
                    "type": "conv2d",
                    "filters": 8,
                    "kernel_size": 5,
                    "stride": 1
                },
                {
                    "type": "conv2d",
                    "filters": 8,
                    "kernel_size": 5,
                    "stride": 1,
                    "batchnorm": true
                },
                {
                    "type": "maxpool2d",
                    "pool_size": 2,
                    "stride": 2
                },
                {
                    "type": "conv2d",
                    "filters": 16,
                    "kernel_size": 5,
                    "stride": 1
                },
                {
                    "type": "conv2d",
                    "filters": 16,
                    "kernel_size": 5,
                    "stride": 1,
                    "batchnorm": true
                },
                {
                    "type": "maxpool2d",
                    "pool_size": 2,
                    "stride": 2
                },
                {
                    "type": "conv2d",
                    "filters": 32,
                    "kernel_size": 5,
                    "stride": 1
                },
                {
                    "type": "conv2d",
                    "filters": 32,
                    "kernel_size": 5,
                    "stride": 1,
                    "batchnorm": true
                },
                {
                    "type": "maxpool2d",
                    "pool_size": 2,
                    "stride": 2
                },
                {
                    "type": "flatten",
                    "kernel_size": 5,
                    "stride": 1
                },
                {
                    "type": "linear",
                    "in_features": "auto",
                    "out_features": 128
                },
                {
                    "type": "dropout",
                    "rate": 0.8
                }
            ]
        },
        "pretrained": {
            "name": "resnet50"
        },
        "lenet": {
            "name": "lenet5"
        }
    },
    "evaluation": {
        "metrics": [
            "accuracy",
            "precision",
            "f1",
            "recall",
            "auc",
            "classification_report"
        ]
    },
    "checkpoints": {
        "dir": "bacil_experiments/run_7/checkpoints"
    },
    "onnx": {
        "dir": "bacil_experiments/run_7/onnx"
    },
    "logging": {
        "interval": 10,
        "wandb": {
            "project": "TB_CLS_CUSTOM"
        },
        "tensorboard": {
            "logdir": "bacil_experiments/run_7/tensorboard_logs"
        }
    }
}